msgstr "I</etc/mtab>"

# type: Plain text
#: C/man8/mount.nfs.8:89 C/man8/umount.nfs.8:63
msgid "table of mounted file systems"
msgstr "Table des systèmes de fichiers montés"

# type: Plain text
