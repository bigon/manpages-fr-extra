#. type: TP
#: C/man8/nfsstat.8:23
#, no-wrap
msgid "B<-n, --nfs>"
msgstr "B<-n, --nfs>"

#. type: Plain text
#: C/man8/nfsstat.8:27
msgid ""
"Print only NFS statistics. The default is to print both NFS and RPC "
