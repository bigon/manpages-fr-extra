msgstr "idmapd.conf 5"

#. type: Plain text
#: C/man5/idmapd.conf.5:9
msgid "E<.Nm idmapd.conf>"
msgstr "E<.Nm idmapd.conf>"

#. type: Nd
#: C/man5/idmapd.conf.5:9
#, no-wrap
msgid "configuration file for idmapd, the NFSv4 ID E<lt>-E<gt> Name Mapper"
