msgstr ""

# type: Plain text
#: C/man8/showmount.8:35
msgid "List only the directories mounted by some client."
msgstr ""

# type: TP
#: C/man8/showmount.8:35
#, no-wrap
msgid "B<-e> or B<--exports>"
msgstr ""

