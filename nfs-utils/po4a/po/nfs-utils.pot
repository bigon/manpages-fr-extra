#: C/man8/mount.nfs.8:61 C/man8/umount.nfs.8:48
msgid "Print help message."
msgstr ""

#. type: TP
#: C/man8/mount.nfs.8:61
#, no-wrap
msgid "B<nfsoptions>"
msgstr ""

