msgid ""
"B<exportfs>(8), B<exports>(5), B<showmount>(8), B<rpc.nfsd>(8), "
"B<rpc.rquotad>(8), B<nfs>(5), B<tcpd>(8), B<hosts_access>(5), "
"B<iptables>(8), B<netconfig>(5)"
msgstr ""

