msgstr ""

#. type: Plain text
#: C/man8/showmount.8:15
msgid ""
"B<showmount> B<[\\ -adehv\\ ]> B<[\\ --all\\ ]> B<[\\ --directories\\ ]> "
"B<[\\ --exports\\ ]> B<[\\ --help\\ ]> B<[\\ --version\\ ]> B<[\\ host\\ ]>"
