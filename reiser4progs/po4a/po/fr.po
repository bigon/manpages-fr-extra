msgid "B<debugfs.reiser4(8),> B<mkfs.reiser4(8),> B<fsck.reiser4(8)>"
msgstr "B<debugfs.reiser4(8),> B<mkfs.reiser4(8),> B<fsck.reiser4(8)>"
