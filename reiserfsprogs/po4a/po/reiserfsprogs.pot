msgid "MKREISERFS"
msgstr ""

#. type: TH
#: C/man8/mkreiserfs.8:4 C/man8/reiserfsck.8:4 C/man8/reiserfstune.8:4 C/man8/resize_reiserfs.8:4
#, no-wrap
