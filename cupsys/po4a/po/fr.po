msgid ""
"B<cancel> [ -E ] [ -U I<username> ] [ -a ] [ -h I<hostname[:port]> ] [ -u "
"I<username> ] [ I<id> ] [ I<destination> ] [ I<destination-id> ]"
msgstr ""
"B<cancel> [ -E ] [ -U I<utilisateur> ] [ -a ] [ -h I<serveur[:port]> ] [ -u "
"I<utilisateur> ] [ I<id> ] [ I<destination> ] [ I<id_destination> ]"

