msgstr ""

# type: Plain text
#: C/man7/filter.7:185
msgid "The name of the printer."
msgstr ""

# type: TP
#: C/man7/filter.7:186
#, no-wrap
msgid "RIP_CACHE"
msgstr ""

# type: Plain text
#: C/man7/filter.7:191
msgid ""
"The recommended amount of memory to use for Raster Image Processors (RIPs)."
msgstr ""

# type: TP
#: C/man7/filter.7:192
#, no-wrap
msgid "SOFTWARE"
msgstr ""

# type: Plain text
#: C/man7/filter.7:196
msgid "The name and version number of the server (typically CUPS/1.2)."
msgstr ""

# type: TP
#: C/man7/filter.7:197
#, no-wrap
msgid "TZ"
msgstr ""

