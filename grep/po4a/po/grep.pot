#: C/man1/grep.1:1211
#, no-wrap
msgid "SEE ALSO"
msgstr ""

# type: SS
#: C/man1/grep.1:1212
#, no-wrap
msgid "Regular Manual Pages"
msgstr ""

# type: Plain text
#: C/man1/grep.1:1219
msgid ""
"awk(1), cmp(1), diff(1), find(1), gzip(1), perl(1), sed(1), sort(1), xargs"
"(1), zgrep(1), mmap(2), read(2), pcre(3), pcrepattern(3), terminfo(5), glob"
"(7), regex(7)."
msgstr ""

