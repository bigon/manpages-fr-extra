#: C/man1/mk_cmds.1:3
#, no-wrap
msgid "MK_CMDS"
msgstr "MK_CMDS"

#. type: TH
#: C/man1/mk_cmds.1:3
#, no-wrap
msgid "2003"
msgstr "2003"

#. type: TH
#: C/man1/mk_cmds.1:3
