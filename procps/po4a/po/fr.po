# type: Plain text
#: C/man1/free.1:11
msgid ""
"B<free >[B<-b> | B<-k> | B<-m> | B<-g>] [B<-o>] [B<-s> I<delay> ] [B<-t>] "
"[B<-V>]"
msgstr ""
"B<free >[B<-b> | B<-k> | B<-m> | B<-g>] [B<-o>] [B<-s> I<délai> ] [B<-t>] "
"[B<-V>]"

# type: SH
#: C/man1/free.1:11 C/man1/kill.1:19 C/man1/pgrep.1:22 C/man1/pmap.1:21
