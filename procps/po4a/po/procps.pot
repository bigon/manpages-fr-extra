#. type: Plain text
#: C/man1/ps.1:122
msgid "B<ps\\ -eLf>"
msgstr ""

#. type: Plain text
#: C/man1/ps.1:124
msgid "B<ps\\ axms>"
msgstr ""

#. type: TP
#: C/man1/ps.1:124
#, no-wrap
msgid "To get security info:"
msgstr ""

#. type: Plain text
#: C/man1/ps.1:127
msgid "B<ps\\ -eo euser,ruser,suser,fuser,f,comm,label>"
msgstr ""

#. type: Plain text
#: C/man1/ps.1:129
msgid "B<ps\\ axZ>"
msgstr ""

#. type: Plain text
#: C/man1/ps.1:131
msgid "B<ps\\ -eM>"
msgstr ""

#. type: TP
#: C/man1/ps.1:131
#, no-wrap
msgid ""
"To see every process running as root (real\\ &\\ effective\\ ID) in user "
"format:"
msgstr ""

#. type: Plain text
