#: C/man1/ptx.1:29
#, no-wrap
msgid "B<-O>, B<--format>=I<roff>"
msgstr "B<-O>, B<--format>=I<roff>"

#. type: Plain text
#: C/man1/ptx.1:32
msgid "generate output as roff directives"
msgstr "générer la sortie comme des directives roff"

#. type: TP
#: C/man1/ptx.1:32
#, no-wrap
