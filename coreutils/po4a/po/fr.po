msgstr ""
"afficher selon le MODE across B<-x>, commas B<-m>, horizontal B<-x>, long B<-"
"l>, single-column B<-1>, verbose B<-l> ou vertical B<-C>"

# type: TP
#: english/dir.1:66 english/vdir.1:66 english/ls.1:66
#, no-wrap
msgid "B<--full-time>"
msgstr "B<--full-time>"

# type: Plain text
#: english/dir.1:69 english/vdir.1:69 english/ls.1:69
msgid "like B<-l> B<--time-style>=I<full-iso>"
msgstr "identique à B<-l> B<--time-style>=I<full-iso>"

# type: TP
#: english/dir.1:69 english/vdir.1:69 english/ls.1:69
#, no-wrap
