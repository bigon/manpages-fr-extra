#: C/man1/cp.1:5
msgid "cp - copy files and directories"
msgstr "cp - Copier des fichiers et des répertoires"

#. type: Plain text
#: C/man1/cp.1:8
msgid "B<cp> [I<OPTION>]... [I<-T>] I<SOURCE DEST>"
msgstr "B<cp> [I<OPTION>]... [I<-T>] I<SOURCE CIBLE>"

