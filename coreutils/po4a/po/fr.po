#: english/shred.1:21
#, no-wrap
msgid "B<-s>, B<--size>=I<N>"
msgstr "B<-s>, B<--size>=I<N>"

# type: Plain text
#: english/shred.1:24
msgid "shred this many bytes (suffixes like K, M, G accepted)"
msgstr "déchiqueter N octets (les suffixes K, M, G sont acceptés)"

# type: TP
#: english/shred.1:24
#, no-wrap
msgid "B<-u>, B<--remove>"
msgstr "B<-u>, B<--remove>"

