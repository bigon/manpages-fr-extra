msgstr "DIRNAME"

#. type: Plain text
#: C/man1/dirname.1:5
msgid "dirname - strip non-directory suffix from file name"
msgstr "dirname - Ne conserver que la partie répertoire d'un nom de fichier"

#. type: Plain text
#: C/man1/dirname.1:8
msgid "B<dirname> I<NAME>"
msgstr "B<dirname> I<NOM>"

#. type: Plain text
#: C/man1/dirname.1:11
