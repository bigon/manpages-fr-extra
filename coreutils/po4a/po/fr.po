msgstr "B<-t>, B<--format>=I<TYPE>"

#. type: Plain text
#: C/man1/od.1:38
msgid "select output format or formats"
msgstr "choisir le(s) format(s) de sortie"

#. type: TP
#: C/man1/od.1:38
#, no-wrap
msgid "B<-v>, B<--output-duplicates>"
msgstr "B<-v>, B<--output-duplicates>"

#. type: Plain text
#: C/man1/od.1:41
msgid "do not use * to mark line suppression"
msgstr "ne pas utiliser * pour indiquer une suppression de ligne"

#. type: TP
#: C/man1/od.1:41
#, no-wrap
msgid "B<-w>, B<--width>[=I<BYTES>]"
msgstr "B<-w>, B<--width>[=I<N>]"

#. type: Plain text
#: C/man1/od.1:44
msgid "output BYTES bytes per output line"
msgstr "afficher N octets par ligne de sortie"

#. type: TP
#: C/man1/od.1:44
#, no-wrap
msgid "B<--traditional>"
msgstr "B<--traditional>"

#. type: Plain text
#: C/man1/od.1:47
msgid "accept arguments in traditional form"
msgstr "accepter les paramètres selon la forme traditionnelle"

#. type: SS
#: C/man1/od.1:53
#, no-wrap
msgid "Traditional format specifications may be intermixed; they accumulate:"
msgstr "Les spécifications traditionnelles de format peuvent être mélangées\\ ; elles se cumulent\\ :"

#. type: Plain text
#: C/man1/od.1:57
