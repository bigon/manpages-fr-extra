#: english/dir.1:156
#, no-wrap
msgid "B<--sort>=I<WORD>"
msgstr "B<--sort>=I<MODE>"

# type: Plain text
#: english/dir.1:161
msgid ""
"extension B<-X>, none B<-U>, size B<-S>, time B<-t>, version B<-v>, status "
