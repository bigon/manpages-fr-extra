# type: TP
#: english/dir.1:59 english/vdir.1:59 english/ls.1:59
#, no-wrap
msgid "B<--file-type>"
msgstr "B<--file-type>"

# type: Plain text
#: english/dir.1:62 english/vdir.1:62 english/ls.1:62
