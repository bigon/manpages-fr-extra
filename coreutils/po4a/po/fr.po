#: english/dir.1:62 english/vdir.1:62 english/ls.1:62
#, no-wrap
msgid "B<--format>=I<WORD>"
msgstr "B<--format>=I<MODE>"

# type: Plain text
#: english/dir.1:66 english/vdir.1:66 english/ls.1:66
msgid ""
"across B<-x>, commas B<-m>, horizontal B<-x>, long B<-l>, single-column B<-"
