msgstr ""

#. type: textblock
#: C/apps/verify.pod:348 C/crypto/X509_STORE_CTX_get_error.pod:184
msgid "the supplied certificate cannot be used for the specified purpose."
msgstr ""

#. type: =item
#: C/apps/verify.pod:350
msgid "B<27 X509_V_ERR_CERT_UNTRUSTED: certificate not trusted>"
msgstr ""

#. type: textblock
#: C/apps/verify.pod:352 C/crypto/X509_STORE_CTX_get_error.pod:188
