msgstr ""

#. type: =item
#: C/apps/verify.pod:277
msgid "B<10 X509_V_ERR_CERT_HAS_EXPIRED: certificate has expired>"
msgstr ""

#. type: textblock
#: C/apps/verify.pod:279 C/crypto/X509_STORE_CTX_get_error.pod:115
msgid ""
