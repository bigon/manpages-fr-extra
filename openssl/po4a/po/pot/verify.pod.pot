msgstr ""

#. type: =item
#: C/apps/verify.pod:285
msgid "B<12 X509_V_ERR_CRL_HAS_EXPIRED: CRL has expired>"
msgstr ""

#. type: textblock
#: C/apps/verify.pod:287 C/crypto/X509_STORE_CTX_get_error.pod:123
msgid "the CRL has expired."
msgstr ""

#. type: =item
#: C/apps/verify.pod:289
msgid ""
