msgstr ""

#. type: =item
#: C/apps/sess_id.pod:92
msgid "B<Protocol>"
msgstr ""

#. type: textblock
#: C/apps/sess_id.pod:94
msgid "this is the protocol in use TLSv1, SSLv3 or SSLv2."
