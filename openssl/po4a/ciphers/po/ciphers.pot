#. type: =head2
#: C/apps/ciphers.pod:389
msgid "TLS v1.0 cipher suites."
msgstr ""

#. type: verbatim
#: C/apps/ciphers.pod:391
#, no-wrap
msgid ""
" TLS_RSA_WITH_NULL_MD5                   NULL-MD5\n"
" TLS_RSA_WITH_NULL_SHA                   NULL-SHA\n"
" TLS_RSA_EXPORT_WITH_RC4_40_MD5          EXP-RC4-MD5\n"
