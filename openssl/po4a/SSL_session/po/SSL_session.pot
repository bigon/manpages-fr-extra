msgstr ""

#. type: textblock
#: C/ssl/SSL_session_reused.pod:32
msgid "A new session was negotiated."
msgstr ""

#. type: =item
#: C/ssl/SSL_session_reused.pod:34
msgid "Z<>1"
msgstr ""

#. type: textblock
#: C/ssl/SSL_session_reused.pod:36
msgid "A session was reused."
msgstr ""

