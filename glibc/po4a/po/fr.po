#, no-wrap
msgid "getconf - Query system configuration variables"
msgstr "getconf - Récupérer les variables de configuration système"

# type: Plain text
#: C/man1/getconf.1:197
msgid "B<getconf> [B<-v specification>] system_var"
msgstr "B<getconf> [B<-v spécification>] variable_système"

# type: Plain text
#: C/man1/getconf.1:199
msgid "B<getconf> [B<-v specification>] path_var pathname"
msgstr "B<getconf> [B<-v specification>] variable_fichier chemin"

# type: TP
#: C/man1/getconf.1:201 C/man1/zdump.1:18 C/man8/zic.8:72
#, no-wrap
msgid "B<-v>"
msgstr "B<-v>"

# type: Plain text
#: C/man1/getconf.1:205
msgid ""
"\\& Indicate the specification and version for which to obtain \\& "
"configuration variables."
msgstr ""
"\\& Indiquer la spécification et la version pour lesquelles il faut obtenir "
"les variables de configuration."

# type: Plain text
#: C/man1/getconf.1:207
msgid "B<system_var>"
msgstr "B<variable_système>"

# type: Plain text
#: C/man1/getconf.1:211
msgid ""
"\\& A system configuration variable, as defined by sysconf(3) or \\& confstr"
"(3)."
msgstr ""
"\\& Une variable de configuration système, comme définie par sysconf(3) ou "
"confstr(3)."

# type: Plain text
#: C/man1/getconf.1:213
msgid "B<path_var>"
msgstr "B<variable_fichier>"

# type: Plain text
#: C/man1/getconf.1:217
msgid ""
"\\& A system configuration variable as defined by pathconf(3). This \\& must "
"be used with a pathname."
msgstr ""
"\\& Une variable de configuration système, comme définie par pathconf(3). Un "
"chemin doit être précisé."

# type: Plain text
#: C/man1/getconf.1:220
msgid "I<getconf> was written by Roland McGrath for the GNU C Library"
msgstr "I<getconf> a été écrit par Roland McGrath pour la bibliothèque GNU C."

