#~ msgid "B<e2undo> [ B<-f> ] I<undo_log device>"
#~ msgstr "B<e2undo> [ B<-f> ] I<journal_undo périphérique>"

#~ msgid ""
#~ "B<e2undo> will replay the undo log I<undo_log> for an ext2/ext3/ext4 "
