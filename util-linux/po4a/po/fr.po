#: english/man5/fstab.5:197
msgid "I</etc/fstab>"
msgstr "I</etc/fstab>"

# type: Plain text
#: english/man5/fstab.5:203
msgid "B<getmntent>(3), B<mount>(8), B<swapon>(8), B<fs>(5)  B<nfs>(5)"
msgstr "B<getmntent>(3), B<mount>(8), B<swapon>(8), B<fs>(5), B<nfs>(5)"

