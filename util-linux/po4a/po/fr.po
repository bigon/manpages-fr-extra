#. type: TP
#: C/man8/mount.8:1973
#, fuzzy, no-wrap
#| msgid "B<nr_inodes=>"
msgid "B<bind:NodeList>"
msgstr "B<nr_inodes=>"

#. type: Plain text
#: C/man8/mount.8:1976
msgid "allocates memory only from nodes in NodeList"
msgstr ""

# type: TP
#. type: TP
#: C/man8/mount.8:1976
#, fuzzy, no-wrap
#| msgid "B<undelete>"
msgid "B<interleave>"
msgstr "B<undelete>"

#. type: Plain text
#: C/man8/mount.8:1979
msgid "prefers to allocate from each node in turn"
