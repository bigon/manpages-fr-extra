msgstr ""

#. type: TP
#: C/man8/ldattach.8:124
#, no-wrap
msgid "B<-2> | B<--twostopbits>"
msgstr ""

#. type: Plain text
#: C/man8/ldattach.8:127
msgid "Sets the number of stop bits of the serial line to two."
msgstr ""

#. type: TP
#: C/man8/ldattach.8:127
#, no-wrap
msgid "B<-i> I<value> | B<--iflag> [B<->]I<value>{,...}"
msgstr ""

#. type: Plain text
