#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: TP
#: C/man1/chrt.1:59 C/man1/taskset.1:72
#, no-wrap
msgid "B<-a>,\\ B<--all-tasks>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:63
msgid ""
"Set or retrieve the scheduling attributes of all the tasks (threads) for a "
"given PID."
msgstr ""

#. type: TP
#: C/man1/chrt.1:63
#, no-wrap
msgid "B<-b>,\\ B<--batch>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:68
msgid ""
"Set scheduling policy to B<SCHED_BATCH> (Linux-specific). The priority "
"argument has to be set to zero."
msgstr ""

#. type: TP
#: C/man1/chrt.1:68
#, no-wrap
msgid "B<-f>,\\ B<--fifo>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:72
msgid "Set scheduling policy to B<SCHED_FIFO>."
msgstr ""

#. type: TP
#: C/man1/chrt.1:72
#, no-wrap
msgid "B<-i>,\\ B<--idle>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:77
msgid ""
"Set scheduling policy to B<SCHED_IDLE> (Linux-specific). The priority "
"argument has to be set to zero."
msgstr ""

#. type: TP
#: C/man1/chrt.1:77
#, no-wrap
msgid "B<-m>,\\ B<--max>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:80
msgid "Show minimum and maximum valid priorities, then exit."
msgstr ""

#. type: TP
#: C/man1/chrt.1:80
#, no-wrap
msgid "B<-o>,\\ B<--other>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:84
msgid "Set policy scheduling policy to B<SCHED_OTHER>."
msgstr ""

#. type: TP
#: C/man1/chrt.1:84 C/man1/taskset.1:80
#, no-wrap
msgid "B<-p>,\\ B<--pid>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:87 C/man1/taskset.1:83
msgid "Operate on an existing PID and do not launch a new task."
msgstr ""

#. type: TP
#: C/man1/chrt.1:87
#, no-wrap
msgid "B<-R>,\\ B<--reset-on-fork>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:96
msgid ""
"Add B<SCHED_RESET_ON_FORK> flag to the B<SCHED_FIFO> or B<SCHED_RR> "
"scheduling policy (Linux-specific)."
msgstr ""

#. type: TP
#: C/man1/chrt.1:96
#, no-wrap
msgid "B<-r>,\\ B<--rr>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:103
msgid ""
"Set scheduling policy to B<SCHED_RR>.  When policy is not defined the "
"B<SCHED_RR> is used as default."
msgstr ""

#. type: TP
#: C/man1/chrt.1:103
#, no-wrap
msgid "B<-v>,\\ B<--verbose>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:106
msgid "Show status information."
msgstr ""

#. type: TP
#: C/man1/chrt.1:106 C/man1/taskset.1:83
#, no-wrap
msgid "B<-V>,\\ B<--version>"
msgstr ""

