msgid "SETSID"
msgstr ""

#. type: TH
#: C/man1/setsid.1:4 C/man8/rdev.8:4
#, no-wrap
msgid "20 November 1993"
msgstr ""

#. type: Plain text
#: C/man1/setsid.1:7
msgid "setsid - run a program in a new session"
msgstr ""

#. type: Plain text
#: C/man1/setsid.1:11
msgid "B<setsid> I<program> [I<arg>...]"
msgstr ""

#. type: Plain text
#: C/man1/setsid.1:14
msgid "B<setsid> runs a program in a new session."
msgstr ""

#. type: Plain text
#: C/man1/setsid.1:16
msgid "B<setsid>(2)"
msgstr ""

#. type: Plain text
#: C/man1/setsid.1:18
msgid "Rick Sladkey E<lt>jrs@world.std.comE<gt>"
msgstr ""

