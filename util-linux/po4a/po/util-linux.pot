msgstr ""

# type: Plain text
#: C/man8/blockdev.8:8
msgid "blockdev - call block device ioctls from the command line"
msgstr ""

# type: Plain text
#: C/man8/blockdev.8:12
msgid "B<blockdev> [I<options>] I<commands devices>"
msgstr ""

# type: Plain text
#: C/man8/blockdev.8:16
msgid ""
"The utility B<blockdev> allows one to call block device ioctls from the "
"command line."
msgstr ""

# type: Plain text
#: C/man8/blockdev.8:20 C/man8/umount.8:59
msgid "Print version and exit."
msgstr ""

# type: TP
#: C/man8/blockdev.8:20 C/man8/ipcs.8:29
#, no-wrap
msgid "B<-q>"
msgstr ""

# type: Plain text
#: C/man8/blockdev.8:23
msgid "Be quiet."
msgstr ""

# type: Plain text
#: C/man8/blockdev.8:26 C/man8/swapon.8:132
msgid "Be verbose."
msgstr ""

# type: TP
#: C/man8/blockdev.8:27
#, no-wrap
msgid "B<--setro>"
msgstr ""

# type: Plain text
#: C/man8/blockdev.8:30
msgid "Set read-only."
msgstr ""

# type: TP
#: C/man8/blockdev.8:30
#, no-wrap
msgid "B<--setrw>"
msgstr ""

# type: Plain text
#: C/man8/blockdev.8:33
msgid "Set read-write."
msgstr ""

# type: TP
#: C/man8/blockdev.8:33
#, no-wrap
msgid "B<--getro>"
msgstr ""

# type: Plain text
#: C/man8/blockdev.8:36
msgid "Get read-only. Print 1 if the device is read-only, 0 otherwise."
msgstr ""

# type: TP
#: C/man8/blockdev.8:36
#, no-wrap
msgid "B<--getss>"
msgstr ""

# type: Plain text
#: C/man8/blockdev.8:39
msgid "Print sectorsize in bytes - usually 512."
msgstr ""

