#. type: TP
#: C/man8/ldattach.8:97 C/man8/rtcwake.8:48
#, no-wrap
msgid "B<-h> | B<--help>"
msgstr ""

#. type: TP
#: C/man8/ldattach.8:100 C/man8/rtcwake.8:51
#, no-wrap
msgid "B<-V> | B<--version>"
msgstr ""

#. type: TP
#: C/man8/ldattach.8:103
#, no-wrap
msgid "B<-s> I<value> | B<--speed> I<value>"
msgstr ""

#. type: Plain text
#: C/man8/ldattach.8:106
msgid "Set the speed of the serial line to the specified value."
msgstr ""

#. type: TP
#: C/man8/ldattach.8:106
#, no-wrap
msgid "B<-7> | B<--sevenbits>"
msgstr ""

#. type: Plain text
#: C/man8/ldattach.8:109
msgid "Sets the character size of the serial line to 7 bits."
msgstr ""

#. type: TP
#: C/man8/ldattach.8:109
#, no-wrap
msgid "B<-8> | B<--eightbits>"
msgstr ""

