msgid "enables libblkid debug output."
msgstr ""

#. type: IP
#: C/man8/cfdisk.8:182 C/man8/fdisk.8:328 C/man8/findmnt.8:255 C/man8/lsblk.8:159 C/man8/sfdisk.8:400
#, no-wrap
msgid "LIBSMARTCOLS_DEBUG=all"
msgstr ""

