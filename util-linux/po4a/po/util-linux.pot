msgstr ""

#. type: TP
#: C/man8/ldattach.8:112
#, no-wrap
msgid "B<-n> | B<--noparity>"
msgstr ""

#. type: Plain text
#: C/man8/ldattach.8:115
msgid "Sets the parity of the serial line to none."
msgstr ""

#. type: TP
#: C/man8/ldattach.8:115
#, no-wrap
msgid "B<-e> | B<--evenparity>"
msgstr ""

#. type: Plain text
#: C/man8/ldattach.8:118
msgid "Sets the parity of the serial line to even."
msgstr ""

#. type: TP
#: C/man8/ldattach.8:118
#, no-wrap
msgid "B<-o> | B<--oddparity>"
msgstr ""

