#. type: TH
#: C/man1/ipcmk.1:3
#, no-wrap
msgid "IPCMK"
msgstr ""

#. type: Plain text
#: C/man1/ipcmk.1:6
msgid "ipcmk - make various IPC resources"
msgstr ""

#. type: Plain text
#: C/man1/ipcmk.1:9
msgid "B<ipcmk> [options]"
msgstr ""

#. type: Plain text
#: C/man1/ipcmk.1:13
msgid ""
"B<ipcmk> allows you to create shared memory segments, message queues, and "
"semaphore arrays."
msgstr ""

#. type: TP
#: C/man1/ipcmk.1:14
#, no-wrap
msgid "Resources can be specified with these options:"
msgstr ""

#. type: TP
#: C/man1/ipcmk.1:16
#, no-wrap
msgid "B<-M>,B< --shmem >I<size>"
msgstr ""

#. type: Plain text
#: C/man1/ipcmk.1:21
msgid "Create a shared memory segment of I<size> bytes."
msgstr ""

#. type: TP
#: C/man1/ipcmk.1:21
#, no-wrap
msgid "B<-Q>,B< --queue>"
msgstr ""

#. type: Plain text
#: C/man1/ipcmk.1:24
msgid "Create a message queue."
msgstr ""

#. type: TP
#: C/man1/ipcmk.1:24
#, no-wrap
msgid "B<-S>,B< --semaphore >I<number>"
msgstr ""

#. type: Plain text
#: C/man1/ipcmk.1:29
msgid "Create a semaphore array with I<number> of elements."
msgstr ""

#. type: Plain text
#: C/man1/ipcmk.1:31
msgid "Other options are:"
msgstr ""

#. type: TP
#: C/man1/ipcmk.1:31
#, no-wrap
msgid "B<-p>,B< --mode >I<mode>"
msgstr ""

#. type: Plain text
#: C/man1/ipcmk.1:34
msgid "Access permissions for the resource.  Default is 0644."
msgstr ""

