"/dev/fd1h1200 (minor = 9)\n"
"/dev/fd1D360  (minor = 13)\n"
"/dev/fd1H360  (minor = 13)\n"
"/dev/fd1D720  (minor = 17)\n"
"/dev/fd1H720  (minor = 17)\n"
"/dev/fd1h360  (minor = 21)\n"
"/dev/fd1h720  (minor = 25)\n"
"/dev/fd1H1440 (minor = 29)\n"
