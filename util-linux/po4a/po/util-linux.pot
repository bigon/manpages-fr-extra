msgstr ""

#. type: TP
#: C/man1/more.1:151
#, no-wrap
msgid "B<\\&/pattern>"
msgstr ""

#. type: Plain text
#: C/man1/more.1:154
msgid "Search for kth occurrence of regular expression.  Defaults to 1."
msgstr ""

#. type: TP
#: C/man1/more.1:154 C/man8/cfdisk.8:94
#, no-wrap
msgid "B<n>"
msgstr ""

#. type: Plain text
#: C/man1/more.1:157
msgid "Search for kth occurrence of last regular expression.  Defaults to 1."
msgstr ""

#. type: TP
#: C/man1/more.1:157
#, no-wrap
msgid "B<!command>\\ orB<\\ :!command>"
msgstr ""

#. type: Plain text
#: C/man1/more.1:162
msgid "Execute I<command> in a subshell."
msgstr ""

#. type: TP
#: C/man1/more.1:162
#, no-wrap
msgid "B<v>"
msgstr ""

#. type: Plain text
#: C/man1/more.1:179
msgid ""
"Start up an editor at current line.  The editor is taken from the "
"environment variable B<VISUAL> if defined, or B<EDITOR> if B<VISUAL> is not "
"defined, or defaults to B<vi> if neither B<VISUAL> nor B<EDITOR> is defined."
msgstr ""

#. type: TP
#: C/man1/more.1:179
#, no-wrap
msgid "B<\\&^L>"
msgstr ""

#. type: Plain text
#: C/man1/more.1:182
msgid "Redraw screen."
msgstr ""

#. type: TP
#: C/man1/more.1:182
#, no-wrap
msgid "B<:n>"
msgstr ""

#. type: Plain text
#: C/man1/more.1:185
msgid "Go to kth next file.  Defaults to 1."
msgstr ""

#. type: TP
#: C/man1/more.1:185
#, no-wrap
msgid "B<:p>"
msgstr ""

#. type: Plain text
#: C/man1/more.1:188
msgid "Go to kth previous file.  Defaults to 1."
msgstr ""

#. type: TP
#: C/man1/more.1:188
#, no-wrap
msgid "B<:f>"
msgstr ""

#. type: Plain text
#: C/man1/more.1:191
msgid "Display current file name and line number."
msgstr ""

#. type: TP
#: C/man1/more.1:191
#, no-wrap
msgid "B<\\&.>"
msgstr ""

#. type: Plain text
#: C/man1/more.1:194
msgid "Repeat previous command."
msgstr ""

#. type: Plain text
#: C/man1/more.1:198
msgid ""
"The B<more> command respects the following environment variables, if they "
"exist:"
msgstr ""

#. type: TP
#: C/man1/more.1:198
#, no-wrap
msgid "B<MORE>"
msgstr ""

#. type: Plain text
#: C/man1/more.1:202
msgid "This variable may be set with favored options to B<more>."
