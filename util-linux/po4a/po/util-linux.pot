msgid "OPTIONS"
msgstr ""

#. type: TP
#: C/man1/chrt.1:63
#, no-wrap
msgid "B<-a, --all-tasks>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:67
msgid ""
"Set or retrieve the scheduling attributes of all the tasks (threads) for a "
"given PID."
msgstr ""

#. type: TP
#: C/man1/chrt.1:67
#, no-wrap
msgid "B<-b, --batch>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:72
msgid "Set scheduling policy to B<SCHED_BATCH> (Linux specific)."
msgstr ""

#. type: TP
#: C/man1/chrt.1:72
#, no-wrap
msgid "B<-f, --fifo>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:76
msgid "Set scheduling policy to B<SCHED_FIFO>."
msgstr ""

#. type: TP
#: C/man1/chrt.1:76
#, no-wrap
msgid "B<-i, --idle>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:81
msgid "Set scheduling policy to B<SCHED_IDLE> (Linux specific)."
msgstr ""

#. type: TP
#: C/man1/chrt.1:81
#, no-wrap
msgid "B<-m, --max>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:84
msgid "Show minimum and maximum valid priorities, then exit."
msgstr ""

#. type: TP
#: C/man1/chrt.1:84
#, no-wrap
msgid "B<-o, --other>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:88
msgid "Set policy scheduling policy to B<SCHED_OTHER>."
msgstr ""

#. type: IP
#: C/man1/chrt.1:88 C/man1/prlimit.1:44
#, no-wrap
msgid "B<-p, --pid>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:91 C/man1/taskset.1:79
msgid "Operate on an existing PID and do not launch a new task."
msgstr ""

#. type: TP
#: C/man1/chrt.1:91
#, no-wrap
msgid "B<-r, --rr>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:98
msgid ""
"Set scheduling policy to B<SCHED_RR>.  When policy is not defined the "
"B<SCHED_RR> is used as default."
msgstr ""

#. type: TP
#: C/man1/chrt.1:98
#, no-wrap
msgid "B<-R, --reset-on-fork>"
msgstr ""

#. type: Plain text
#: C/man1/chrt.1:107
msgid ""
"Add B<SCHED_RESET_ON_FORK> flag to the B<SCHED_FIFO> or B<SCHED_RR> "
"scheduling policy (Linux specific)."
msgstr ""

