msgstr "OPTIONS"

#. type: TP
#: C/man1/ldd.1:21
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: C/man1/ldd.1:25
msgid "Print the version number of B<ldd>."
msgstr "Affiche le numéro de version de B<ldd>."

#. type: TP
#: C/man1/ldd.1:25 C/man8/ldconfig.8:57
#, no-wrap
msgid "B<-v\\ --verbose>"
msgstr "B<-v\\ --verbose>"

#. type: Plain text
#: C/man1/ldd.1:28
msgid "Print all information, including e.g. symbol versioning information."
msgstr "Affiche toutes les informations, y compris les versions des symboles."

#. type: TP
#: C/man1/ldd.1:28
#, no-wrap
msgid "B<-d\\ --data-relocs>"
msgstr "B<-d\\ --data-relocs>"

#. type: Plain text
#: C/man1/ldd.1:31
msgid "Perform relocations and report any missing objects (ELF only)."
msgstr ""
"Réalise les déplacements et indique les objets manquants (ELF seulement)."

#. type: TP
#: C/man1/ldd.1:31
#, no-wrap
msgid "B<-r\\ --function-relocs>"
msgstr "B<-r\\ --function-relocs>"

#. type: Plain text
#: C/man1/ldd.1:35
msgid ""
"Perform relocations for both data objects and functions, and report any "
"missing objects or functions (ELF only)."
msgstr ""
"Réalise les déplacements pour les objets donnés et les fonctions, et indique "
"les objets ou fonctions manquants (ELF seulement)."

#. type: TP
#: C/man1/ldd.1:35
#, no-wrap
msgid "B<-u\\ --unused>"
msgstr "B<-u\\ --unused>"

#. type: Plain text
#: C/man1/ldd.1:38
msgid "Print unused direct dependencies."
msgstr "Affiche les dépendances directes non utilisée."

