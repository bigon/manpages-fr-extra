# type: Plain text
#: english/man8/activate.8:23
msgid "activate - read/write flags marking active boot partition"
msgstr "activate - Lire/écrire des drapeaux activant une partition d'amorce"

# type: SH
#: english/man8/activate.8:23
#, no-wrap
msgid "SYNOPSIS"
msgstr "SYNOPSIS"

# type: Plain text
#: english/man8/activate.8:26
msgid "B</sbin/activate> I<device>"
